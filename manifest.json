{
  "manifest_version": 2,

  "name": "Black to Color",
  "description": "This extension will turn black and white websites back to color",
  "version": "1.3.0",

  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },

  "browser_action": {
   "default_icon": "img/icon.png"
  },
  "permissions": [
   "activeTab"
   ],
   "content_scripts": [
     {
       "matches": ["*://*/*"],
       "css": ["css/styles.css"],
       "js": [
           "js/jquery.min.js",
         "js/app.js"
       ]
     }
   ]
}
